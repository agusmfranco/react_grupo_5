 response.json()\r\n        })\r\n        .then(response => {\r\n            this.setState({response: response.data})\r\n        })\r\n        .catch(error => console.log(error))\r\n    }\r\n\r\n    render(){\r\n    return (\r\n        <React.Fragment>\r\n        \r\n        <h1 onClick= {this.mensaje} className=\"h3 mb-2 text-gray-800\">Todos los productos en la base de datos</h1>\r\n        \r\n        \r\n        <div className=\"card shadow mb-4\">\r\n            <div className=\"card-body\">\r\n                <div className=\"table-responsive\">\r\n                    <table className=\"table table-bordered\" id=\"dataTable\" width=\"100%\" cellspacing=\"0\">\r\n                        <thead>\r\n                            <tr>\r\n                                <th>Id</th>\r\n                                <th>Titulo</th>\r\n                                <th>Sinopsis</th>\r\n                            </tr>\r\n                        </thead>\r\n                        <tfoot>\r\n                            <tr>\r\n                                <th>Id</th>\r\n                                <th>Titulo</th>\r\n                                <th>Sinopsis</th>\r\n                            </tr>\r\n                        </tfoot>\r\n                        <tbody>\r\n                            {\r\n                                this.state.response.map((producto, index) => {\r\n                                    return <ProductList {...producto} key={index}/>\r\n                                })\r\n                            }\r\n                        </tbody>\r\n                    </table>\r\n                </div>\r\n            </div>\r\n    </div>\r\n    \r\n    </React.Fragment>\r\n    )\r\n}\r\n}\r\nexport default Product;\r\n","C:\\Users\\Agus\\Desktop\\react\\practica-react\\src\\components\\ContentRowBottom.js",[],"C:\\Users\\Agus\\Desktop\\react\\practica-react\\src\\components\\SmallCard.js",[],"C:\\Users\\Agus\\Desktop\\react\\pract